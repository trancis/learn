}
.myRadioLabel
{
display:table-cell;
vertical-align: middle;

}

input[type='radio'].myRadio:hover {
    box-shadow:0 0 5px 0px orange inset;
}
input[type='radio'].myRadio:before {
    content:'';
    display:block;
    width:100%;
    height:100%;
    margin: 20% auto;    
    border-radius:50%;
    margin-top:0px;
     
}
input[type='radio'].myRadio:checked:before {
    background:rgb(255,127,80);
}
