div .confirm
{
    display:none;
}
div .confirm +label
{
    color:white;
    font-size:14px;
    padding: 4px 0px 0px 3px;
    text-align:center;
    cursor:pointer
}
.confirm#yes + label
{
    display:inline-block;
    height:25px;
    width:45px;
    background:grey;
    border-top-left-radius:45%;
    border-bottom-left-radius:45%;
}
.confirm#maybe + label
{
    display:inline-block;
    height:25px;
    width:50px;
    background:grey;
}
.confirm#no + label
{
    display:inline-block;
    height:25px;
    width:45px;
    background:grey;
border-top-right-radius:45%;
    border-bottom-right-radius:45%;
}
.confirm:checked + label
{
    background:red;
}